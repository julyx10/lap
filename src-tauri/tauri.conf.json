{
  "build": {
    "devPath": "http://localhost:3580",
    "distDir": "../src-vite/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "jc-photo",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "create": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": false
      },
      "shell": {
        "all": false,
        "open": true
      },
      "os": {
        "all": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "jc-photo",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.julyxx.jc-photo",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
