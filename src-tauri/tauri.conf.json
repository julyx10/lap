{
  "productName": "Lap",
  "version": "0.1.5",
  "identifier": "com.julyx10.lap",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:3580",
    "frontendDist": "../src-vite/dist"
  },
  "app": {
    "withGlobalTauri": true,
    "macOSPrivateApi": true,
    "windows": [
      {
        "label": "main",
        "title": "",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "visible": false,
        "transparent": true
      }
    ],
    "security": {
      "capabilities": [
        "default"
      ],
      "assetProtocol": {
        "enable": true,
        "scope": [
          "$HOME/**",
          "$PICTURES/**",
          "$VIDEOS/**",
          "$DOCUMENT/**",
          "$DESKTOP/**",
          "$DOWNLOAD/**",
          "/Volumes/**",
          "/media/**",
          "/mnt/**",
          "/run/media/**"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ],
    "resources": [
      "resources/models/*"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/julyx10/lap/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDUzMEQ0QzM2RTRDOUNEMjUKUldRbHpjbmtOa3dOVXlBYm0rYk9lY2FtVGpzWTJ2NkRqdUtSOEN4eUxwRlVTaUlVbkJ4aHhQS3gK"
    }
  }
}
