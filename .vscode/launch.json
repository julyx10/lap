  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Tauri",
      "cwd": "${workspaceFolder}/src-tauri",  // Set the working directory
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "sourceLanguages": ["rust"],
      "env": {
        "RUST_BACKTRACE": "1"
      },
      "cargo": {
        "args": [
          "build",
          "--manifest-path=./src-tauri/Cargo.toml",
          "--no-default-features"
        ]
      },
      // task for the `beforeDevCommand` if used, must be configured in `.vscode/tasks.json`
      // "preLaunchTask": "ui:dev"
    },
    // {
    //   "type": "node",
    //   "request": "launch",
    //   "name": "Debug Vite",
    //   "program": "${workspaceFolder}/src-vite/node_modules/.bin/vite",
    //   "args": ["--config", "src-vite/vite.config.js"],
    //   "cwd": "${workspaceFolder}/src-vite",
    //   "runtimeExecutable": null,
    //   "console": "integratedTerminal"
    // }
  ]
}